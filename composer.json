{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4.0",
        "ext-json": "*",
        "additionapps/flexible-presenter": "^2.0",
        "anlutro/l4-settings": "^1.0",
        "bluemmb/faker-picsum-photos-provider": "^2.0",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "friendsofphp/php-cs-fixer": "3.0.x-dev",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/laravel-sign-in-with-apple": "^0.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3",
        "jenssegers/model": "^1.2",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "lcobucci/jwt": "^3.4",
        "orchestra/parser": "^6.0",
        "oshomo/csv-utils": "^6.0",
        "owen-it/laravel-auditing": "^12.0",
        "parsedown/laravel": "^1.2",
        "pusher/pusher-php-server": "^5.0",
        "reinink/advanced-eloquent": "^0.2.0",
        "reinink/remember-query-strings": "^0.1.0",
        "seld/jsonlint": "^1.7",
        "silber/bouncer": "v1.0.0-rc.10",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-personal-data-export": "^1.3",
        "spatie/laravel-translatable": "^4.2",
        "tightenco/ziggy": "^1.0.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/tinkerwell-helper": "^1.1",
        "fakerphp/faker": "^1.13",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
